{
  "name": "campertunity-mcp-server",
  "version": "0.0.4",
  "description": "MCP Server for Campertunity - A Model Context Protocol server for interacting with camping and outdoor recreation data",
  "type": "module",
  "bin": {
    "mcp-campertunity-server": "dist/index.js"
  },
  "files": [

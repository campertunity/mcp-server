  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/campertunity/mcp-server"
  },
  "bugs": {
    "url": "https://github.com/campertunity/mcp-server/issues"
  },
  "homepage": "https://campertunity.com",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.7.0",
    "@types/node": "^22"
  },
  "devDependencies": {
    "@types/node": "^22",
    "shx": "^0.3.4",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}